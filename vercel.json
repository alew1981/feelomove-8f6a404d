{
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/sitemap:path*.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=index"
    },
    {
      "source": "/sitemap_index.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=index"
    },
    {
      "source": "/sitemap-pages.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=pages"
    },
    {
      "source": "/sitemap-concerts.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=concerts"
    },
    {
      "source": "/sitemap-festivals.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=festivals"
    },
    {
      "source": "/sitemap-artists.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=artists"
    },
    {
      "source": "/sitemap-destinations.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=destinations"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
