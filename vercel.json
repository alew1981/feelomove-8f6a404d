{
  "redirects": [
    {
      "source": "/events/:id",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/product/:slug/feed/",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/product/:slug/",
      "destination": "/producto/:slug",
      "statusCode": 301
    },
    {
      "source": "/product/:slug",
      "destination": "/producto/:slug",
      "statusCode": 301
    },
    {
      "source": "/attractions",
      "destination": "/artistas",
      "statusCode": 301
    },
    {
      "source": "/user-profile/",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/_next/:path*",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/musica",
      "destination": "/generos",
      "statusCode": 301
    },
    {
      "source": "/musica/:genero",
      "destination": "/generos/:genero",
      "statusCode": 301
    },
    {
      "source": "/artista/:slug",
      "destination": "/conciertos/:slug",
      "statusCode": 301
    },
    {
      "source": "/conciertos/:slug/:city",
      "destination": "/conciertos/:slug",
      "statusCode": 301
    },
    {
      "source": "/festivales/:slug/:city",
      "destination": "/festivales/:slug",
      "statusCode": 301
    },
    {
      "source": "/conciertos/:slug(.*u.*u.*)",
      "destination": "/eventos",
      "statusCode": 301
    },
    {
      "source": "/festivales/:slug(.*u.*u.*)",
      "destination": "/eventos",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap_index.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=index"
    },
    {
      "source": "/sitemap-pages.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=pages"
    },
    {
      "source": "/sitemap-concerts.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=concerts"
    },
    {
      "source": "/sitemap-festivals.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=festivals"
    },
    {
      "source": "/sitemap-artists.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=artists"
    },
    {
      "source": "/sitemap-destinations.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=destinations"
    },
    {
      "source": "/sitemap-genres.xml",
      "destination": "https://wcyjuytpxxqailtixept.supabase.co/functions/v1/sitemap?type=genres"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/((?!api|sitemap.*\\.xml|robots\\.txt).*)",
      "destination": "/index.html"
    }
  ]
}